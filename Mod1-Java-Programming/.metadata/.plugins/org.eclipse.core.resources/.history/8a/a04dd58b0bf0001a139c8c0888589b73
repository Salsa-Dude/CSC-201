
public class Chap5Ex10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int counter = 0;
		
		for(int i = 100; i <= 1000; i++) {
			if(counter == 10) {
				System.out.println("--------------------");
				counter = 0;
			}
			else if(i % 5 == 0 && i % 6 == 0) {
				System.out.print(i);
				counter++;
			}
		}
	}

}
